{
    "name": "szyminson/simple-website",
    "type": "project",
    "description": "A simple micro-framework for simple websites.",
    "keywords": ["simple","website", "szyminson", "micro", "framework"],
    "homepage": "https://github.com/szyminson/simple-website",
    "license": "MIT",
    "require": {
        "eftec/bladeone": "^3.17",
        "vlucas/phpdotenv": "^2.5",
        "oomphinc/composer-installers-extender": "^1.1"
    },
    "authors": [
        {
            "name": "szyminson",
            "email": "szyminson@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {"SimpleWebsite\\": "classes/"}
    },
    "extra": {
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "public/assets/{$name}/": ["type:bower-asset", "type:npm-asset"]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
